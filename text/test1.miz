  then
  A34: a9, c9 // p, q by A28, A29, AFF_1: 5;
  A35: p <> b & p <> b9 & q <> b & q <> b9
  proof
    A36:
    now
      assume
      A36: b9 = q;
      (not LIN b9, c9, a9) & c9, a9 // q, p by A21, A34, AFF_1: 4, 6;
