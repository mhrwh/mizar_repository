      A92: LIN b, a, p and
      A93: LIN b9, a9, p and
      A94: LIN b, c, q and
      A95: LIN b9, c9, q and
      A96: a, c // a9, c9;
      A97: LIN p, a, b by A92, AFF_1: 6;
      set A9 = Line (a, c), P9 = Line (p, q), C9 = Line (a9, c9);
      A98: q in P9 by A90, AFF_1: 24;
      A99: a <> p
      proof
        assume a = p;
        then LIN a, a9, b9 by A93, AFF_1: 6;
        then b9 in A by A76, A81, A91, AFF_1: 25;
        hence contradiction by A78, A83, A86, AFF_1: 45;
      end;
      A100: not LIN p, a, a9
      proof
        A101: LIN p, a, a by AFF_1: 7;
        assume LIN p, a, a9;
        then b in A by A76, A81, A91, A99, A97, A101, AFF_1: 8, 25;
        hence contradiction by A78, A82, A86, AFF_1: 45;
      end;
      A102: LIN q, c9, b9 & LIN p, a9, b9 by A93, A95, AFF_1: 6;
      A103: a <> c by A88, AFF_1: 7;
      then
      A104: A9 is being_line by AFF_1: 24;
      A105: C // P by A86, A87, AFF_1: 44;
      then
      A106: c, c9 // b, b9 by A82, A83, A84, A85, AFF_1: 39;
      A107: c <> c9
      proof
        assume c = c9;
        then c, a // c, a9 by A96, AFF_1: 4;
        then LIN c, a, a9 by AFF_1:def 1;
        then LIN a, a9, c by AFF_1: 6;
        then c in A by A76, A81, A91, AFF_1: 25;
        hence contradiction by A79, A84, A87, AFF_1: 45;
      end;
      A108: b <> b9
      proof
        A109: LIN b, p, a & LIN b, p, b by A92, AFF_1: 6, 7;
        assume
        A110: b = b9;
        then LIN b, p, a9 by A93, AFF_1: 6;
        then
        A111: b = p or b in A by A76, A81, A91, A109, AFF_1: 8, 25;
        A112: LIN b, q, c & LIN b, q, b by A94, AFF_1: 6, 7;
        LIN b, q, c9 by A95, A110, AFF_1: 6;
        then b = q or b in C by A77, A84, A85, A107, A112, AFF_1: 8, 25;
        hence contradiction by A78, A80, A82, A86, A90, A105, A111, AFF_1: 45;
      end;
      A113: a in A9 & c in A9 by A103, AFF_1: 24;
      A114: P9 is being_line & c, c9 // a, a9 by A81, A84, A85, A87, A90,
      AFF_1: 24, 39;
      A115: p in P9 by A90, AFF_1: 24;
      A116: a9 <> c9 by A89, AFF_1: 7;
      then
      A117: a9 in C9 by AFF_1: 24;
      A118: A9 <> C9
      proof
        assume A9 = C9;
        then LIN a, a9, c by A104, A113, A117, AFF_1: 21;
        then c in A by A76, A81, A91, AFF_1: 25;
        hence contradiction by A79, A84, A87, AFF_1: 45;
      end;
      A119: q <> c
      proof
        assume q = c;
        then LIN c, c9, b9 by A95, AFF_1: 6;
        then b9 in C by A77, A84, A85, A107, AFF_1: 25;
        hence contradiction by A80, A83, A105, AFF_1: 45;
      end;
      A120: A9 <> P9
      proof
        assume A9 = P9;
        then
        A121: LIN c, q, a & LIN c, q, c by A104, A113, A98, AFF_1: 21;
        LIN c, q, b by A94, AFF_1: 6;
        hence contradiction by A88, A119, A121, AFF_1: 8;
      end;
      A122: LIN q, c, b by A94, AFF_1: 6;
      A123: not LIN q, c, c9
      proof
        A124: LIN q, c, c by AFF_1: 7;
        assume LIN q, c, c9;
        then b in C by A77, A84, A85, A107, A119, A122, A124, AFF_1: 8, 25;
        hence contradiction by A80, A82, A105, AFF_1: 45;
      end;
      A125: C9 is being_line & c9 in C9 by A116, AFF_1: 24;
      then A9 // C9 by A96, A103, A116, A104, A113, A117, AFF_1: 38;
      then A9 // P9 by A75, A103, A108, A104, A113, A115, A98, A117, A125,
      A114, A106
      , A122, A97, A102, A120, A118, A123, A100;
      hence thesis by A113, A115, A98, AFF_1: 39;
    end;
  end;
  hence thesis by A1;
end;

theorem
  AP is satisfying_DES1_3 implies AP is satisfying_DES2_1
proof
  assume
  A1: AP is satisfying_DES1_3;
  let A, P, C, a, a9, b, b9, c, c9, p, q such that
  A2: A is being_line and
  A3: P is being_line and
  A4: C is being_line and
  A5: A <> P and
  A6: A <> C and
  A7: P <> C and
  A8: a in A and
  A9: a9 in A and
  A10: b in P and
  A11: b9 in P and
  A12: c in C and
  A13: c9 in C and
  A14: A // P and
  A15: A // C and
  A16: not LIN b, a, c and
  A17: not LIN b9, a9, c9 and
  A18: p <> q and
  A19: LIN b, a, p and
  A20: LIN b9, a9, p and
  A21: LIN b, c, q and
  A22: LIN b9, c9, q and
  A23: a, c // p, q;
  A24: P // C by A14, A15, AFF_1: 44;
  set K = Line (p, q), M = Line (a, c), N = Line (a9, c9);
  A25: a <> c by A16, AFF_1: 7;
  then
  A26: a in M by AFF_1: 24;
  A27: c, c9 // a, a9 & LIN q, c, b by A8, A9, A12, A13, A15, A21, AFF_1: 6,
  39;
  A28: LIN p, a9, b9 by A20, AFF_1: 6;
  C // P by A14, A15, AFF_1: 44;
  then
  A29: c, c9 // b, b9 by A10, A11, A12, A13, AFF_1: 39;
  A30: LIN q, c9, b9 & LIN p, a, b by A19, A22, AFF_1: 6;
  A31: c in M by A25, AFF_1: 24;
  assume
  A32: not thesis;
  A33: c <> q
  proof
    assume
    A34: c = q;
    then c, a // c, p by A23, AFF_1: 4;
    then LIN c, a, p by AFF_1:def 1;
    then
    A35: LIN p, a, c by AFF_1: 6;
    LIN p, a, b & LIN p, a, a by A19, AFF_1: 6, 7;
    then p = a by A16, A35, AFF_1: 8;
    then LIN a, a9, b9 by A20, AFF_1: 6;
    then
    A36: a = a9 or b9 in A by A2, A8, A9, AFF_1: 25;
    LIN c, c9, b9 by A22, A34, AFF_1: 6;
    then c = c9 or b9 in C by A4, A12, A13, AFF_1: 25;
    hence contradiction by A5, A7, A11, A14, A32, A24, A36, AFF_1: 2, 45;
  end;
  A37: c <> c9
  proof
    A38:
    now
      assume
      A38: p = b;
      LIN b, q, c by A21, AFF_1: 6;
      then b, q // b, c by AFF_1:def 1;
      then a, c // b, c or b = q by A23, A38, AFF_1: 5;
      then c, a // c, b by A18, A38, AFF_1: 4;
      then LIN c, a, b by AFF_1:def 1;
      hence contradiction by A16, AFF_1: 6;
    end;
    A39: LIN q, c, b & LIN q, c, c by A21, AFF_1: 6, 7;
    assume
    A40: c = c9;
    then LIN q, c, b9 by A22, AFF_1: 6;
    then b = b9 or c in P by A3, A10, A11, A33, A39, AFF_1: 8, 25;
    then
    A41: LIN p, b, a9 by A7, A12, A20, A24, AFF_1: 6, 45;
    LIN p, b, a & LIN p, b, b by A19, AFF_1: 6, 7;
    then a = a9 or b in A by A2, A8, A9, A41, A42, AFF_1: 8, 25;
    hence contradiction by A5, A10, A14, A32, A40, AFF_1: 2, 45;
  end;
  A43: b <> b9
  proof
    assume b = b9;
    then
    A44: LIN q, b, c9 by A22, AFF_1: 6;
    LIN q, b, c & LIN q, b, b by A21, AFF_1: 6, 7;
    then
    A45: q = b or b in C by A4, A12, A13, A37, A44, AFF_1: 8, 25;
    b, a // b, p by A19, AFF_1:def 1;
    then a, b // p, b by AFF_1: 4;
    then a, c // a, b or p = b by A7, A10, A23, A24, A45, AFF_1: 5, 45;
    then LIN a, c, b by A7, A10, A18, A24, A45, AFF_1: 45,def 1;
    hence contradiction by A16, AFF_1: 6;
  end;
  A46: not LIN q, c, c9
  proof
    assume
    A47: LIN q, c, c9;
    LIN q, c, b & LIN q, c, c by A21, AFF_1: 6, 7;
    then b in C by A4, A12, A13, A33, A37, A47, AFF_1: 8, 25;
    hence contradiction by A7, A10, A24, AFF_1: 45;
  end;
  A48: a9 <> c9 by A17, AFF_1: 7;
  then
  A49: N is being_line by AFF_1: 24;
  A50: p <> a
  proof
    assume p = a;
    then LIN a, c, q by A23, AFF_1:def 1;
    then
    A51: LIN c, q, a by AFF_1: 6;
    LIN c, q, b & LIN c, q, c by A21, AFF_1: 6, 7;
    hence contradiction by A16, A33, A51, AFF_1: 8;
  end;
  A52: not LIN p, a, a9
  proof
    assume
    A53: LIN p, a, a9;
    LIN p, a, b & LIN p, a, a by A19, AFF_1: 6, 7;
    then a = a9 or b in A by A2, A8, A9, A50, A53, AFF_1: 8, 25;
    then
    A54: LIN p, a, b9 by A5, A10, A14, A20, AFF_1: 6, 45;
    LIN p, a, b & LIN p, a, a by A19, AFF_1: 6, 7;
    then a in P by A3, A10, A11, A43, A50, A54, AFF_1: 8, 25;
    hence contradiction by A5, A8, A14, AFF_1: 45;
  end;
  A55: M is being_line by A25, AFF_1: 24;
  A56: K is being_line & q in K by A18, AFF_1: 24;
  A57:
  now
    assume M = K;
    then
    A57: LIN q, c, a & LIN q, c, c by A56, A26, A31, AFF_1: 21;
    LIN q, c, b by A21, AFF_1: 6;
    hence contradiction by A16, A33, A57, AFF_1: 8;
  end;
  A58: c9 in N by A48, AFF_1: 24;
  A59: a9 in N by A48, AFF_1: 24;
  then consider x such that
  A60: x in M and
  A61: x in N by A32, A55, A49, A26, A31, A58, AFF_1: 39, 58;
  A63:
  now
    assume x = c;
    then N = C by A4, A12, A13, A37, A49, A58, A61, AFF_1: 18;
    hence contradiction by A6, A9, A15, A59, AFF_1: 45;
  end;
  A62: p in K by A18, AFF_1: 24;
  then
  A63: M // K by A18, A23, A25, A55, A56, A26, A31, AFF_1: 38;
  A66:
  now
    assume x = c9;
    then M = C by A4, A12, A13, A37, A55, A31, A60, AFF_1: 18;
    hence contradiction by A6, A8, A15, A26, AFF_1: 45;
  end;
  M <> N by A32, A55, A26, A31, A59, A58, AFF_1: 51;
  then x in K by A1, A18, A25, A55, A49, A62, A56, A26, A31, A59, A58, A60,
  A61, A29, A27, A30
  , A28, A43, A64, A65, A46, A52;
  hence contradiction by A63, A60, A66, AFF_1: 45;
end;
